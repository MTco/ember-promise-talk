<div class="col-md-8 col-md-offset-2">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title text-center">Synchronous input/output</h3>
    </div>
    <div class="panel-body">
      {{#ember-highlight-code language="javascript" addLineNumbers=true}}
        function foo(input = 'input') {
          return `${input}/output`;
        }

        foo(); // =&gt; "input/output"
      {{/ember-highlight-code}}
    </div>
  </div>
</div>
